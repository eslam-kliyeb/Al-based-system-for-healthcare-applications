@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>AI</title>
    <!-- Main Template CSS File -->
    <link rel="stylesheet" href="~/css/leon.css" />
    <!-- Render All Elements Normally -->
    <link rel="stylesheet" href="~/css/normalize.css" />
    <!-- Font Awesome Library -->
    <link rel="stylesheet" href="~/css/all.min.css" />
    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com" />
    <link
      href="https://fonts.googleapis.com/css2?family=Work+Sans:wght@200;300;400;500;600;700;800&display=swap"
      rel="stylesheet"
    />
    <script type="text/javascript" src="/HTML_And_CSS_Template_One-main/javascript/javascript.js"></script>
  </head>
  <body>
    <!-- Start Header -->
    <div class="header" >
      <div class="container">
            <img class="logo" src="~/css/images/logo.png" alt="" />
        <div class="links">
          <span class="icon">
            <span></span>
            <span></span>
            <span></span>
          </span>
          <ul>
            <li><a href="#home">Home</a></li>
            <li><a href="#services">Services</a></li>
            <li><a href="#about">About</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
        </div>
      </div>
    </div>
    <!-- End Header -->
    <!-- Start Landing Section -->
    <div class="landing" id="home">
      <div class="arrow">
        <span></span>
        <span></span>
        <span></span>
      </div>
      <div class="intro-text">
        <h1><span>hello </span> dear <span>doctor</span> </h1>
            <p>Welcome to BrainTumorAI, the future of brain tumor diagnosis. Our advanced AI tool is designed to 
                assist radiologists and healthcare professionals in accurately classifying and segmenting brain 
                tumors with unparalleled precision. <span class="quotes">”<span></p>
      </div>
      <br>
      <!--Start boutton-->
      <div id="wrapper">
            <a href="@Url.Action("Login", "Admin")" class="my-super-cool-btn">
          <div class="dots-container">
            <div class="dot"></div>
            <div class="dot"></div>
            <div class="dot"></div>
            <div class="dot"></div>
          </div>
          <span>Go!</span>
        </a>
      </div>
      <!--End boutton-->
    </div>
    <!-- End Landing Section -->
    <!-- Start Services -->
    <div class="services" id="services">
      <div class="container">
        <h2 class="special-heading">Services</h2>
        <div class="services-content">
          <div class="col">
            <!-- Start Service -->
            <div class="srv">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke=" #fa658e"
                             stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-crop">
                            <path d="M6.13 1L6 16a2 2 0 0 0 2 2h15"></path>
                            <path d="M1 6.13L16 6a2 2 0 0 1 2 2v15"></path>
                        </svg>
              <div class="text">
                <h3>Brain Tumor localization</h3>
                <p>
                   Achieve accurate localization, enabling precise identification
                   of tumor boundaries for better treatment planning.
                </p>
              </div>
            </div>
            <div class="srv">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#fa658e"
                             stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-message-square">
                            <path d="M21 15a2 2 0 0 1-2 2H7l-4 4V5a2 2 0 0 1 2-2h14a2 2 0 0 1 2 2z"></path>
                        </svg>
              <div class="text">
                <h3>Chat Bot</h3>
                <p>
                  Our AI chatbot helps create thorough and accurate radiology reports.
                  Automatically generates detailed descriptions and insights based on the scan results.
                  Ensures consistency and accuracy, saving time and reducing errors.
                </p>
              </div>
            </div>
            <!-- End Service -->
          </div>
          <div class="col">
            <!-- Start Services -->
            <div class="srv">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke=" #fa658e"
                        stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-image">
                        <rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect>
                        <circle cx="8.5" cy="8.5" r="1.5"></circle>
                        <polyline points="21 15 16 10 5 21"></polyline>
                        </svg>
              <div class="text">
                <h3>Brain Tumor classification</h3>
                <p>
                  Utilize state-of-the-art AI algorithms to analyze MRI scans 
                  and distinguish between different types of brain tumors.
                </p>
              </div>
            </div>
            <!-- End Services -->
          </div>
          <div class="col">
            <div class="image image-column">
            <img src="~/css/images/to.jpg" alt="" />
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- End Services -->
    <!-- Start About -->
    <div class="about" id="about">
      <div class="container">
        <h2 class="special-heading">About</h2>
        <div class="about-content">
          <div class="image">
                    <img src="~/css/images/about.png" alt="" />
          </div>
          <div class="text">
            <p>
               Why Choose BrainTumorAI?
            </p>
            <hr />
            <p>
             <ul>
                <li >
                 Accuracy: Leveraging cutting-edge AI technology for reliable and precise tumor analysis.
                </li>
                <li>
                 Efficiency: Streamline the radiology workflow with automated report generation and easy-to-use tools.
                </li>
                <li>
                 Collaboration: Enhance teamwork and communication with shareable reports and real-time insights.
                </li>
                <li>
                 Innovation: Stay at the forefront of medical technology with our continuously evolving AI solutions.
                </li>
             </ul>
            </p>
          </div>
        </div>
      </div>
    </div>
    <!-- End About -->
    <!-- Start Contact -->
    <div class="contact"  id="contact">
      <div class="container">
        <h2 class="special-heading">Contact</h2>
        <div class="info">
          <p class="label">Feel free to drop us a line at:</p>
                <a href="mailto:braintumora@gmail.com?subject=Contact" class="link">aibrain@mail.com</a>
        </div>
      </div>
    </div>
    <!-- End Contact -->
    <!-- Start Footer -->
    <div class="footer">&copy; 2024 <span>MTI</span> All Right Reserved</div>
    <!-- End Footer -->
  </body>
</html>
