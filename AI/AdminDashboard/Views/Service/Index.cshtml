          href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@48,400,1,0" />
    <script src="script.js" defer></script>
</head>
<body>
    <!-- partial:index.partial.html -->
    <nav class="navbar">
        <ul class="navbar__menu">
            <li class="navbar__item">
                <a href="@Url.Action("classification", "Service", new { Email = Model.Email, Password = Model.Password })" class="navbar__link"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="grey" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-image"><rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect><circle cx="8.5" cy="8.5" r="1.5"></circle><polyline points="21 15 16 10 5 21"></polyline></svg><span>classification</span></a>
            </li>
            <li class="navbar__item">
                <a href="@Url.Action("Segmentation", "Service", new { Email = Model.Email, Password = Model.Password })" class="navbar__link"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="grey" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-crop"><path d="M6.13 1L6 16a2 2 0 0 0 2 2h15"></path><path d="M1 6.13L16 6a2 2 0 0 1 2 2v15"></path></svg><span>localization</span></a>
            </li>
            <li class="navbar__item">
                <a href="@Url.Action("ChatBot", "Service", new { Email = Model.Email, Password = Model.Password })" class="navbar__link"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="grey" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-message-square"><path d="M21 15a2 2 0 0 1-2 2H7l-4 4V5a2 2 0 0 1 2-2h14a2 2 0 0 1 2 2z"></path></svg><span>ChatBot</span></a>
            </li>
            <li class="navbar__item">
                <a href="@Url.Action("Create", "Service", @Model)" class="navbar__link"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="grey" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-user-plus"><path d="M16 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path><circle cx="8.5" cy="7" r="4"></circle><line x1="20" y1="8" x2="20" y2="14"></line><line x1="23" y1="11" x2="17" y2="11"></line></svg><span>AddPatient</span></a>
            </li>
            <li class="navbar__item">
                <a href="@Url.Action("Patient", "Service" , @Model)" class="navbar__link"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="grey" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-file-text"><path d="M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z"></path><polyline points="14 2 14 8 20 8"></polyline><line x1="16" y1="13" x2="8" y2="13"></line><line x1="16" y1="17" x2="8" y2="17"></line><polyline points="10 9 9 9 8 9"></polyline></svg><span>Patients</span></a>
            </li>
            <li class="navbar__item">
                <a href="@Url.Action("MyAccount", "Service" , @Model)" class="navbar__link"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="gray" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-sliders"><line x1="4" y1="21" x2="4" y2="14"></line><line x1="4" y1="10" x2="4" y2="3"></line><line x1="12" y1="21" x2="12" y2="12"></line><line x1="12" y1="8" x2="12" y2="3"></line><line x1="20" y1="21" x2="20" y2="16"></line><line x1="20" y1="12" x2="20" y2="3"></line><line x1="1" y1="14" x2="7" y2="14"></line><line x1="9" y1="8" x2="15" y2="8"></line><line x1="17" y1="16" x2="23" y2="16"></line></svg><span>Account</span></a>
            </li>
        </ul>
    </nav>
    <!-- partial -->
    <!-- Start About -->
    <div class="about" id="about">
        <div class="container">
            <div class="dr"><h2><span>Hello </span> Dear <span>Doctor</span></h2></div>
            <h2 class="special-heading"></h2>
            <div class="about-content">
                <div class="image">
                    <img src="~/css/images/about.png" alt="" />
                </div>
                <div class="text">
                    <p>
                        Why Choose BrainTumorAI?
                    </p>
                    <hr />
                    <p>
                        <ul>
                            <li>
                                Accuracy: Leveraging cutting-edge AI technology for reliable and precise tumor analysis.
                            </li>
                            <li>
                                Efficiency: Streamline the radiology workflow with automated report generation and easy-to-use tools.
                            </li>
                            <li>
                                Collaboration: Enhance teamwork and communication with shareable reports and real-time insights.
                            </li>
                            <li>
                                Innovation: Stay at the forefront of medical technology with our continuously evolving AI solutions.
                            </li>
                    </p>
                </div>
            </div>
        </div>
    </div>
    <!-- End About -->
    <!-- Start Services -->
    <div class="services" id="services">
        <div class="container">
            <h2 class="special-heading">Services</h2>
            <div class="services-content">
                <div class="col">
                    <!-- Start Service -->
                    <div class="srv">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke=" #fa658e"
                             stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-crop">
                            <path d="M6.13 1L6 16a2 2 0 0 0 2 2h15"></path>
                            <path d="M1 6.13L16 6a2 2 0 0 1 2 2v15"></path>
                        </svg>
                        <div class="text">
                            <h3>Brain Tumor localization</h3>
                            <p>
                                Achieve accurate localization, enabling precise identification
                                of tumor boundaries for better treatment planning.
                            </p>
                        </div>
                    </div>
                    <div class="srv">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#fa658e"
                             stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-message-square">
                            <path d="M21 15a2 2 0 0 1-2 2H7l-4 4V5a2 2 0 0 1 2-2h14a2 2 0 0 1 2 2z"></path>
                        </svg>
                        <div class="text">
                            <h3>Chat Bot</h3>
                            <p>
                              Our AI chatbot helps create thorough and accurate radiology reports.
                              Automatically generates detailed descriptions and insights based on the scan results.
                              Ensures consistency and accuracy, saving time and reducing errors.
                            </p>
                        </div>
                    </div>
                    <!-- End Service -->
                </div>
                <div class="col">
                    <!-- Start Services -->
                    <div class="srv">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke=" #fa658e"
                             stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-image">
                            <rect x="3" y="3" width="18" height="18" rx="2" ry="2"></rect>
                            <circle cx="8.5" cy="8.5" r="1.5"></circle>
                            <polyline points="21 15 16 10 5 21"></polyline>
                        </svg>
                        <div class="text">
                            <h3>Brain Tumor classification</h3>
                            <p>
                                Utilize state-of-the-art AI algorithms to analyze MRI scans
                                and distinguish between different types of brain tumors.
                            </p>
                        </div>
                    </div>
                    <!-- End Services -->
                </div>
                <div class="col">
                    <div class="image image-column">
                        <img src="~/css/images/to.jpg" alt="" />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Services -->
    <script src='https://unpkg.com/feather-icons'></script>
    <script src="./script.js"></script> 




