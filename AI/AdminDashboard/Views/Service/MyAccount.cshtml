@model MyAccountViewModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="~/css/fontsSignup/material-icon/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/css/Signupstyle.css">
    <title>Register</title>
</head>
<body>
    <div class="main">
        <section class="signup">
            <div class="container">
                <div class="signup-content">
                    <div class="signin-form">
                        <h2 class="form-title">Account</h2>

                            <div class="form-group">
                                <label for="your_name"><i class="zmdi zmdi-account material-icons-name"></i></label>
                                <input readonly asp-for="@Model.doctorDto.Name" class="form-control" id="your_name" type="DisplayName" placeholder="Name" />
                            </div>

                            <div class="form-group">
                                <label for="your_name"><i class="zmdi zmdi-email"></i></label>
                                <input readonly asp-for="@Model.doctorDto.Email" class="form-control" id="your_name" type="Email" placeholder="Email" />
                            </div>

                            <div class="form-group">
                                <label for="your_name"><i class="zmdi zmdi-phone-in-talk"></i></label>
                            <input readonly asp-for="@Model.doctorDto.Country" class="form-control" id="your_name" type="PhoneNumber" placeholder="Phone Number" />
                            </div>

                            <div class="form-group">
                                <label for="your_name"><i class="zmdi zmdi-pin"></i></label>
                                <input readonly asp-for="@Model.doctorDto.City" class="form-control" id="your_name" type="City" placeholder="City" />
                            </div>

                            <div class="form-group">
                                <label for="your_name"><i class="zmdi zmdi-nature"></i></label>
                            <input readonly asp-for="@Model.doctorDto.Phone" class="form-control" id="your_name" type="Street" placeholder="Street" />
                            </div>

                            <div class="form-group">
                                <label for="your_name"><i class="zmdi zmdi-gps-dot"></i></label>
                                <input readonly asp-for="@Model.doctorDto.Age" class="form-control" id="your_name" type="Country" placeholder="Country" />
                            </div>

                            <div class="form-group">
                                <label for="your_name"><i class="zmdi zmdi-border-color"></i></label>
                                <input readonly asp-for="@Model.doctorDto.Qualification" class="form-control" id="your_name" type="Qualification" placeholder="Qualification" />
                            </div>

                        <a href="@Url.Action("Index", "Service" , @Model.loginDto)" class="btn btn-secondary">Back To List</a>
                    </div>
                    
                    <div class="signup-image">
                        <figure><img src="~/css/images/about.png" alt="sing up image"></figure>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/main.js"></script>
</body>
</html>